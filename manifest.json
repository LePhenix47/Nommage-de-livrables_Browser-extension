{
  "name": "Nommages des livrables",
  "description": "Changer mettre le nom et date pour les livrables sur la plateforme OpenClassrooms",
  "version": "1.2.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/Logo_deliverables-16.png",
    "48": "images/Logo_deliverables-48.png",
    "128": "images/Logo_deliverables-128.png"
  },
  "options_page": "html/options.html",
  "action": {
    "default_icon": "images/Logo_deliverables-48.png",
    "default_popup": "html/pop-up.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://openclassrooms.com/fr/paths/*",
        "https://openclassrooms.com/en/paths/*",
        "https://openclassrooms.com/fr/users/*/paths/*",
        "https://openclassrooms.com/en/users/*/paths/*"
      ]
    }
  ]
}
